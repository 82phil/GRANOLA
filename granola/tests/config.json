                }
            ]
        }
    },

    "stick_hook": {
        "canned_queries": {
            "data": {
                "`DEFAULT`": "data/cereal_cmds.csv",
                "sdicmd 1 get-measurement": "data\\sensor_get_reading_cmds.csv",
