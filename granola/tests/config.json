{
    "cereal": {
        "unsupported_response": "ERROR\r>",

        "canned_queries": { "data": {
            "`DEFAULT`": "data/cereal_cmds.csv",
            "sdicmd 1 get-measurement": "data\\sensor_get_reading_cmds.csv",
            "sdicmd 1": "data\\sensor_cmds.csv"
            }},
        "getters_and_setters": {
            "default_values": {
                "devtype": "Cereal",
                "sn": "42",
                "fw_ver":"0.0.0",
                "temp": "20.0",
                "volts": "2000.0"
            },
            "getters": [
                {
                    "cmd": "get -sn\r",
                    "response": "{{ sn }}\r>"
                },
                {
                    "cmd": "get ver\r",
                    "response": "{{ fw_ver }}\r>"
                },
                {
                    "cmd": "show\r",
                    "response": "{{ devtype }} {{ fw_ver }} {{ sn }}\r>"
                },
                {
                    "cmd": "get -temp\r",
                    "response": "{{ temp }}\r>"
                },
                {
                    "cmd": "get -tempf\r",
                    "response": "{{ temp|float * (9/5) + 32 }}\r>"
                },
                {
                    "cmd": "get volt_temp\r",
                    "response": "{{ (temp|float + volts|float) / 2 }}"
                }
            ],
            "setters": [
                {
                    "cmd": "set -sn {{sn}}\r",
                    "response": "OK\r>"
                },
                {
                    "cmd": "set -some dummy command {{ sn }} {{ fw_ver }}\r",
                    "response": "OK\r>"
                },
                {
                    "cmd": "set -temp {{ temp }}\r",
                    "response": "{{ temp }}"
                },
                {
                    "cmd": "set volt {{ volts }}\r",
                    "response": "Volt Calculation {{ volts|float / 2 }}"
                }
            ]
        }
    },

    "just_getters_and_setters": {

        "getters_and_setters": {
            "default_values": {
                "sn": "42",
                "fw_ver":"123"
            },
            "getters": [
                {
                    "cmd": "get -sn\r",
                    "response": "`sn`\r>"
                },
                {
                    "cmd": "show\r",
                    "response": "SOMEMACHINE `fw_ver` `sn`\r>"
                }
            ],
            "setters": [
                {
                    "cmd": "set -sn `sn`\r",
                    "response": "OK\r>"
                }
            ],
            "variable_start_string": "`",
            "variable_end_string": "`"
        }
    },

